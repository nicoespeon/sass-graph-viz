{
  "name": "sass-graph-viz",
  "version": "2.2.1",
  "description": "Draw a visual graph of Sass dependencies",
  "repository": "nicoespeon/sass-graph-viz",
  "homepage": "https://github.com/nicoespeon/sass-graph-viz",
  "bugs": "https://github.com/nicoespeon/sass-graph-viz/issues",
  "keywords": [
    "sass",
    "dependencies",
    "graph",
    "visual",
    "dependency-graph",
    "dataviz"
  ],
  "bin": {
    "sassgraphviz": "dist/src/cli.js",
    "sgv": "dist/src/cli.js"
  },
  "files": [
    "src",
    "tsconfig.json"
  ],
  "author": "Nicolas Carlo <nicolascarlo.espeon@gmail.com>",
  "license": "MIT",
  "main": "dist/src/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "chmod +x dist/src/cli.js && npm run copy-static-files",
    "copy-static-files": "copyup src/rendering/**/*.{js,css,pug,json} dist/src",
    "build:watch": "npm run prebuild && tsc-watch --onSuccess \"npm run postbuild\"",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "examples": "run-s examples:*",
    "examples:basic-tree": "ts-node examples/basic-tree",
    "examples:without-externals": "ts-node examples/without-externals",
    "examples:focus-on-file": "ts-node examples/focus-on-file",
    "examples:two-entry-points": "ts-node examples/two-entry-points",
    "examples:nested-folders": "ts-node examples/nested-folders",
    "examples:circular-reference": "ts-node examples/circular-reference",
    "examples:orphan-files": "ts-node examples/orphan-files",
    "examples:simple-viz": "ts-node examples/simple-viz",
    "examples:single-file": "ts-node examples/single-file",
    "examples:large-graph": "ts-node examples/large-graph",
    "examples:debug": "ts-node examples/debug"
  },
  "devDependencies": {
    "@types/commander": "2.12.2",
    "@types/compression": "0.0.36",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.1",
    "@types/node": "10.9.4",
    "@types/pug": "2.0.4",
    "@types/sass-graph": "2.1.2",
    "@types/vis": "4.21.7",
    "copyfiles": "2.1.0",
    "jest": "23.5.0",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.2",
    "pretty-quick": "1.6.0",
    "rimraf": "2.6.2",
    "ts-jest": "23.1.4",
    "ts-node": "7.0.1",
    "tsc-watch": "1.0.27",
    "typescript": "3.0.3"
  },
  "dependencies": {
    "chalk": "2.4.1",
    "commander": "2.18.0",
    "compression": "1.7.3",
    "express": "4.16.3",
    "opn": "5.3.0",
    "pug": "2.0.3",
    "sass-graph": "3.0.0",
    "viz.js": "2.0.0"
  }
}
